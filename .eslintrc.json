{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "@typescript-eslint", "react-hooks"],
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "rules": {
    "react/prop-types": "off",
    "no-console": "warn"
  }
}
